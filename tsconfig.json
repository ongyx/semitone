{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2023",
		"lib": ["es2023"],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"rootDir": ".",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitOverride": true,
		"noPropertyAccessFromIndexSignature": true,
		"strictNullChecks": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
